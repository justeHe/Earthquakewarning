}
.type-tabs .tab-item.active text.data-v-61b16127 {
  color: #3b82f6;
}
.type-tabs .tab-item .tab-icon.data-v-61b16127 {
  width: 48rpx;
  height: 48rpx;
  margin-bottom: 8rpx;
}
.type-tabs .tab-item text.data-v-61b16127 {
  font-size: 24rpx;
  color: #666;
  white-space: nowrap;
}
.location-card.data-v-61b16127 {
  background: #ffffff;
  border-radius: 16rpx;
